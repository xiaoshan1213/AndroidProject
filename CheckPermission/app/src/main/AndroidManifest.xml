<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sam.checkpermission" >

    <permission android:name="com.example.sam.checkpermission.permission.SAYHELLO" />
    <permission android:name="com.example.sam.checkpermission.permission.MYATY" />

    <uses-permission android:name="com.example.sam.checkpermission.permission.SAYHELLO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyAty"
            android:label="@string/title_activity_my_aty"
            android:theme="@style/AppTheme.NoActionBar"
            android:permission="com.example.sam.checkpermission.permission.MYATY">
        </activity>
    </application>

</manifest>
